{
  "name": "ts-intl",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.3.0",
    "eslint-config-custom": "workspace:*",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "latest",
    "ts-jest": "^29.0.3",
    "turbo": "latest",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.3"
}
