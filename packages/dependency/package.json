{
  "name": "@ts-intl/dependency",
  "version": "2.0.0",
  "author": "https://github.com/hakonleung",
  "repository": {
    "type": "git",
    "url": "https://github.com/ts-intl/ts-intl.git",
    "directory": "packages/dependency"
  },
  "description": "I18n keys dependency analysis",
  "keywords": [
    "i18n",
    "internationalization",
    "react",
    "dependencies",
    "ssr",
    "typescript",
    "jsx",
    "tsx"
  ],
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/esm/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "license": "MIT",
  "homepage": "",
  "scripts": {
    "build": "tsc && tsc --module commonjs --outDir dist/cjs",
    "watch": "chokidar 'src/**/*.ts' -c 'pnpm -wc exec turbo run build --filter=dependency'",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "ts-check": "tsc --noEmit"
  },
  "dependencies": {
    "madge": "^5.0.1",
    "@ts-intl/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/madge": "^5.0.0",
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=7.0.0"
  }
}
